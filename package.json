{
  "name": "jbart5-react",
  "version": "0.2.5",
  "description": "jBart - Visual Development with react",
  "homepage": "https://github.com/artwaresoft/jbart5-react",
  "bugs": {
    "url": "https://github.com/artwaresoft/jbart5-react/issues"
  },
  "main": "dist/jbart-core.js",
  "files": [
    "dist","bin"
  ],  
  "contributors": [
    "Shai Ben-Yehuda <shaiby@artwaresoft.com>",
    "Yaniv Ben-Yehuda <yanivby@artwaresoft.com>",
    "Yaron Ben-Yehuda <yaronby@artwaresoft.com>"
  ],
  "scripts": {
    "start": "npm run studio",
    "electron": "node_modules/.bin/electron hosts/jb-electron",
    "eStudio": "node_modules/.bin/electron hosts/jb-electron -path:projects/studio/cards",
    "eTests": "node_modules/.bin/electron hosts/jb-electron -path:projects/ui-tests/tests.html",
    "dev": "concurrently \"npm run webpack\" \"npm run studio\" ",
    "build": "node_modules/.bin/webpack & node bin/build.phase2",
    "studio": "npm run openStudio & node jb-studio-server.js",
    "openStudio": "start http://localhost:8082/projects/ui-tests/tests.html",
    "tgp2ts": "node bin/tgp2ts"
  },
  "bin": { "tgp2ts" : "bin/tgp2ts.js" },
  "dependencies": {
    "codemirror": "^5.25.2",
    "dragula": "^3.7.2",
    "history": "^4.6.1",
    "immutability-helper": "^2.1.2",
    "material-design-lite": "^1.3.0",
    "preact": "^8.1.0",
    "rxjs": "^5.3.0",
    "wspy": "github:wix-incubator/wSpy"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-preact-h-to-jsx": "^1.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-standalone": "^6.24.0",
    "concurrently": "^2.2.0",
    "electron": "^1.6.11",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "jison": "^0.4.17",
    "pretty": "^2.0.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArtwareSoft/jbart5-react.git"
  },
  "keywords": [
    "react",
    "jbart",
    "jbart5",
    "visual",
    "visual develpoment"
  ],
  "author": "Shai Ben-Yehuda (http://artwaresoft.com)",
  "license": "SEE LICENSE IN license.txt"
}
