{
	"name": "jbart-studio",
	"displayName": "jBart Studio",
	"description": "jBart Visual Editor",
	"version": "0.6.9",
	"publisher": "artwaresoft",
	"engines": {
		"vscode": "^1.50.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"icon": "logo128.png",
	"keywords": ["javascript", "visual programming", "visual", "html5", "web development", "css"],
	"repository": "https://github.com/Artwaresoft/jb-react",
	"activationEvents": [
		"onView:jbart.jbEditor",
		"onCommand:jb.studio.openJbEditor"
	],
	"main": "vscode-ext.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
			  {
				"id": "jbart",
				"title": "jBart",
				"icon": "logo128.png"
			  }
			]
		},		
		"views": {
			"jbart": [
				{
					"type": "webview",
					"id": "jbart.jbEditor",
					"name": "inteliscript"
				}
			]
		},		
		"commands": [
			{
				"command": "jb.studio.openJbEditor",
				"title": "Open jBart Editor",
				"category": "jBart"
			}
		],
		"keybindings": [
			{
			  "command": "jb.studio.openJbEditor",
			  "key": "ctrl+shift+j",
			  "when": "editorTextFocus"
			}
		]		
	},
	"scripts": {
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"tslint": "^5.19.0",
		"typescript": "^3.8.3",
		"@types/vscode": "^1.38.0"
	}
}
