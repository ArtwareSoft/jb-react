{
	"name": "jbart-studio",
	"displayName": "jBart Studio",
	"description": "jBart Visual Editor",
	"version": "0.6.9",
	"publisher": "artwaresoft",
	"engines": {
		"vscode": "^1.50.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Other"
	],
	"icon": "logo128.png",
	"keywords": [
		"javascript",
		"visual programming",
		"visual",
		"html5",
		"web development",
		"css"
	],
	"repository": "https://github.com/Artwaresoft/jb-react",
	"activationEvents": [
		"onView:jbart.jbEditor",
		"onView:jbart.preview",
		"onView:jbart.logs"
	],
	"main": "vscode-loader.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "jbart",
					"title": "jBart",
					"icon": "logo128.png"
				}
			]
		},
		"views": {
			"jbart": [
				{
					"id": "jbart.jbEditor",
					"type": "webview",
					"name": "inteliscript"
				},
				{
					"id": "jbart.logs",
					"type": "webview",
					"name": "logs"
				},
				{
					"id": "jbart.preview",
					"type": "webview",
					"name": "preview"
				}
			]
		},
		"commands": [
			{
				"command": "jbart.formatComponent",
				"title": "format jBart component"
			}
		],
		"keybindings": [
			{
				"command": "jbart.onEnter",
				"key": "Ctrl+Enter",
				"when": "jbart.inComponent"
			}
		]
	},
	"scripts": {},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"tslint": "^5.19.0",
		"typescript": "^3.8.3",
		"@types/vscode": "^1.38.0"
	}
}
