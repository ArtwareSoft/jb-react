{
	"name": "tgp-lang",
	"displayName": "TGP Language",
	"description": "TGP language service and probe activation",
	"version": "0.9.0",
	"publisher": "artwaresoft",
	"license": "MIT",
	"engines": {
		"vscode": "^1.75.1"
	},
	"icon": "logo128.png",
	"repository": "https://github.com/Artwaresoft/jb-react",
	"activationEvents": [
		"onLanguage:javascript"
	],
	"dependencies": {
		"ws": "^8.13.0"
	},
	"main": "vscode-tgp-lang.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "jbart",
					"title": "jBart",
					"icon": "logo128.png"
				}
			]
		},
		"views": {
			"jbart": [
				{
					"id": "jbart.main",
					"type": "webview",
					"name": "main"
				}
			]
		},
		"commands": [
			{
				"command": "jbart.applyCompChange",
				"title": "apply component change from text editor"
			},
			{
				"command": "jbart.restartLangServer",
				"title": "restart tgp lang completion server"
			},
			{
				"command": "jbart.gotoPath",
				"title": "goto path"
			},
			{
				"command": "jbart.moveUp",
				"title": "move item up in array"
			},
			{
				"command": "jbart.moveDown",
				"title": "move item down in array"
			},
			{
				"command": "jbart.openPreviewPanel",
				"title": "open or rerun preview"
			},
			{
				"command": "jbart.openProbeResultPanel",
				"title": "inspect probe result input output"
			}
		],
		"keybindings": [
			{
				"command": "jbart.moveUp",
				"key": "ctrl+Up",
				"when": "editorTextFocus"
			},
			{
				"command": "jbart.moveDown",
				"key": "ctrl+Down",
				"when": "editorTextFocus"
			},
			{
				"command": "jbart.openPreviewPanel",
				"key": "ctrl+Alt+P",
				"when": "editorTextFocus"
			},
			{
				"command": "jbart.openProbeResultPanel",
				"key": "ctrl+Alt+I",
				"when": "editorTextFocus"
			}
		]
	}
}
