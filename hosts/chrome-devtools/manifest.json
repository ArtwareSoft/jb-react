{
  "name": "jBart in chrome tools",
  "version": "0.1",
  "manifest_version": 2,
  "description": "hello world example in panel dev tools",
  "homepage_url": "http://www.artwaresoft.com",

  "devtools_page": "devtools.html",

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [ "hello-world/hello-world.html" ],

  "permissions": [
    "file:///*",
    "http://*/*",
    "https://*/*"
  ]
}