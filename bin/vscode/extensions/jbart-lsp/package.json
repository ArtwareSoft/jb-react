{
	"name": "jbart-lsp",
	"version": "1.0.0",
	"description": "jbart TGP langauge",
	"author": "Shai Ben-Yehuda",
	"license": "MIT",
	"publisher": "artwaresoft",
	"categories": [],
	"keywords": [
		"jbart tgp ui builder low coding"
	],
	"scripts": {
		"compile": "webpack"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"activationEvents": [
		"onLanguage:jbart"
	],
	"browser": "./client/dist/browserClientMain",
	"contributes": {
		"languages": [
			{
				"id": "jbart",
				"extensions": [
					".jb",
					".tgp"
				],
				"aliases": [
					"jBart",
					"tgp"
				],
				"configuration": "./javascript-language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "jbart",
				"scopeName": "source.js",
				"path": "./syntaxes/JavaScript.tmLanguage.json"
			},
			{
				"scopeName": "source.js.regexp",
				"path": "./syntaxes/Regular Expressions (JavaScript).tmLanguage"
			}
		],
		"configuration": [
			{
				"order": 22,
				"id": "lsp-web-extension-sample",
				"title": "lsp-web-extension-sample",
				"properties": {
					"lsp-web-extension-sample.trace.server": {
						"type": "string",
						"scope": "window",
						"enum": [
							"off",
							"messages",
							"verbose"
						],
						"default": "verbose",
						"description": "Traces the communication between VS Code and the lsp-web-extension-sample language server."
					}
				}
			}
		]
	},
	"devDependencies": {
		"path-browserify": "^1.0.1",
		"ts-loader": "^9.2.3",
		"typescript": "^4.4.3",
		"webpack": "^5.44.0",
		"webpack-cli": "^4.7.2"
	}
}
